			LvQuest = 2
			NameMon = "Desert Officer"

			CFrameMon = CFrame.new(1572, 10, 4373)
			VectorMon = Vector3.new(1572, 10, 4373)

			CFrameQuest = CFrame.new(1572, 10, 4373)
			VectorQuest = Vector3.new(1572, 10, 4373)
		elseif Lv == 90 or Lv <= 99 then -- Snow Bandits
			 

			Name = "Snow Bandit [Lv. 90]"
			NameQuest = "SnowQuest"

			LvQuest = 1
			NameMon = "Snow Bandits"

			CFrameMon = CFrame.new(1289, 150, -1442)
			VectorMon = Vector3.new(1289, 106, -1442)

			CFrameQuest = CFrame.new(1386, 87, -1297)
			VectorQuest = Vector3.new(1386, 87, -1297)
		elseif Lv == 100 or Lv <= 119 then -- Snowman
			 

			Name = "Snowman [Lv. 100]"
			NameQuest = "SnowQuest"

			LvQuest = 2
			NameMon = "Snowman"

			CFrameMon = CFrame.new(1289, 150, -1442)
			VectorMon = Vector3.new(1289, 106, -1442)

			CFrameQuest = CFrame.new(1386, 87, -1297)
			VectorQuest = Vector3.new(1386, 87, -1297)
		elseif Lv == 120 or Lv <= 149 then -- Chief Petty Officer
			 

			Name = "Chief Petty Officer [Lv. 120]"
			NameQuest = "MarineQuest2"

			LvQuest = 1
			NameMon = "Chief Petty Officer"

			CFrameMon = CFrame.new(-4855, 23, 4308)
			VectorMon = Vector3.new(-4855, 23, 4308)

			CFrameQuest = CFrame.new(-5036, 29, 4325)
			VectorQuest = Vector3.new(-5036, 29, 4325)
		elseif Lv == 150 or Lv <= 174 then -- Sky Bandit
			 

			Name = "Sky Bandit [Lv. 150]"
			NameQuest = "SkyQuest"

			LvQuest = 1
			NameMon = "Sky Bandit"

			CFrameMon = CFrame.new(-4981, 278, -2830)
			VectorMon = Vector3.new(-4981, 278, -2830)

			CFrameQuest = CFrame.new(-4842, 718, -2623)
			VectorQuest = Vector3.new(-4842, 718, -2623)
		elseif Lv == 175 or Lv <= 189 then -- Dark Master
			 
			Name = "Dark Master [Lv. 175]"
			NameQuest = "SkyQuest"

			LvQuest = 2
			NameMon = "Dark Master"

			CFrameMon = CFrame.new(-5250, 389, -2272)
			VectorMon = Vector3.new(-5250, 389, -2272)

			CFrameQuest = CFrame.new(-4842, 718, -2623)
			VectorQuest = Vector3.new(-4842, 718, -2623)
		elseif Lv == 190 or Lv <= 209 then -- Dark Master
			 

			Name = "Prisoner [Lv. 190]"
			NameQuest = "PrisonerQuest"

			LvQuest = 1
			NameMon = "Prisoner"

			CFrameMon = CFrame.new(5411, 96, 690)
			VectorMon = Vector3.new(5411, 96, 690)

			CFrameQuest = CFrame.new(5308, 2, 474)
			VectorQuest = Vector3.new(5308, 2, 474)
		elseif Lv == 210 or Lv <= 249 then -- Dark Master
			 

			Name = "Dangerous Prisoner [Lv. 210]"
			NameQuest = "PrisonerQuest"

			LvQuest = 2
			NameMon = "Dangerous Prisoner"

			CFrameMon = CFrame.new(5411, 96, 690)
			VectorMon = Vector3.new(5411, 96, 690)

			CFrameQuest = CFrame.new(5308, 2, 474)
			VectorQuest = Vector3.new(5308, 2, 474)
		elseif Lv == 250 or Lv <= 299 then -- Toga Warrior
			 

			Name = "Toga Warrior [Lv. 250]"
			NameQuest = "ColosseumQuest"

			LvQuest = 1
			NameMon = "Toga Warrior"

			CFrameMon = CFrame.new(-1824, 50, -2743)
			VectorMon = Vector3.new(-1824, 50, -2743)

			CFrameQuest = CFrame.new(-1576, 8, -2985)
			VectorQuest = Vector3.new(-1576, 8, -2985)
		elseif Lv == 300 or Lv <= 329 then -- Military Soldier
			 

			Name = "Military Soldier [Lv. 300]"
			NameQuest = "MagmaQuest"

