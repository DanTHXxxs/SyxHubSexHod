updo:Label("-")

local Main = serv:Channel("General", "http://www.roblox.com/asset/?id=6034287522")


Time = Main:Label("Server Time : ")

function UpdateTime()
    local GameTime = math.floor(workspace.DistributedGameTime+0.5)
    local Hour = math.floor(GameTime/(60^2))%24
    local Minute = math.floor(GameTime/(60^1))%60
    local Second = math.floor(GameTime/(60^0))%60
    Time:Refresh("Hour : "..Hour.." Minute : "..Minute.." Second : "..Second)
end

spawn(function()
    while true do
        UpdateTime()
        wait()
    end
end)

Main:Line()

Main:Label("\                                        \\\\  Main  //")



function click()
   game:GetService'VirtualUser':CaptureController()
   game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
end


 function TP(P)
   local Distance = (P.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude -- จุดที่จะไป Position Only
   local Speed = 300 -- ความเร็วของมึง
   tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear)
   tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = P})
   tween:Play()
 end




Main:Toggle("Auto Farm Level",false,function(t)
	do
    local NM = game:GetService("Workspace"):FindFirstChild("Noclip")
    if NM then
        NM:Destroy()
    end
end
local Noclip = Instance.new("Part",workspace)
      Noclip.Name         = "Noclip"
      Noclip.CanCollide   = t --สารถทำเปลี่ยนเป็นfalseได้
      Noclip.Anchored     = t --สารถทำเปลี่ยนเป็นfalseได้
      Noclip.Transparency = 1
      Noclip.Size         = Vector3.new(50,2,50)
function Noclip()
game:GetService("Workspace"):FindFirstChild("Noclip").CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
end
_G.AutoFarmLevel = t
while _G.AutoFarmLevel do wait(.1)
pcall(function()
Noclip()
end)
end 
end)



function EquipWeapon(ToolSe)
		if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
			local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
			wait(.4)
			game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
		end
	end


	Weapon = {}
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
		if v:IsA("Tool") then
			table.insert(Weapon ,v.Name)
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do  
		if v:IsA("Tool") then
			table.insert(Weapon, v.Name)
		end
	end
	local SearchWeapon = Main:Dropdown("Select Weapon",Weapon,function(Value)
		_G.SearchWeapon = Value
	end)
	Main:Button("Refresh Weapon",function()
		SearchWeapon:Clear()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
			if v:IsA("Tool") then
				SearchWeapon:Add(v.Name)
			end
		end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do  
			if v:IsA("Tool") then
				SearchWeapon:Add(Weapon)
			end
		end
	end) 

	function equip()
		pcall(function()
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				if v.Name == _G.SearchWeapon then
					game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
				end
			end
		end)
	end

Main:Line()

