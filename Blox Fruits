			VectorMon = Vector3.new(932, 7, 4484)

			CFrameQuest = CFrame.new(897, 7, 4388)
			VectorQuest = Vector3.new(897, 7, 4388)
		elseif Lv == 75 or Lv <= 89 then -- Desert Officre
			 

			Name = "Desert Officer [Lv. 70]"
