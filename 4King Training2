    local d = game:GetService("ReplicatedStorage").Price.Diamond.Value
    local e = game:GetService("ReplicatedStorage").Price.Gold.Value
    local f = game:GetService("ReplicatedStorage").Price.Ruby.Value
    Timesa:SetText("üè™ ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏Ñ‡∏≤‡∏ï‡∏•‡∏≤‡∏î‡πÉ‡∏ô‡∏≠‡∏µ‡∏Å: "..Second)
    Timesb:SetText("Pikachu ‡∏£‡∏≤‡∏Ñ‡∏≤ : "..a)
    Timesc:SetText("Coal ‡∏£‡∏≤‡∏Ñ‡∏≤ : "..b)
    Timeszs:SetText("Iron ‡∏£‡∏≤‡∏Ñ‡∏≤ : "..c)
    Timesd:SetText("Diamond ‡∏£‡∏≤‡∏Ñ‡∏≤ : "..d)
    Timese:SetText("Gold ‡∏£‡∏≤‡∏Ñ‡∏≤ : "..e)
    Timesf:SetText("Ruby ‡∏£‡∏≤‡∏Ñ‡∏≤ : "..f)
